<aura:component>
    
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    
    <aura:attribute name="fileIds" type="String[]" />
    
    <aura:iteration items="{!v.fileIds}" var="item">
        <lightning:fileCard fileId="{!item}" description="Sample Description"/>
    </aura:iteration>
    
    
</aura:component>