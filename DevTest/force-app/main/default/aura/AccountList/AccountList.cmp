<aura:component controller="AccountList" >
    
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    
    <aura:attribute name="accounts" type="Account[]" />
    
     <div class="container">
    	<div class="row">
            <aura:iteration items="{!v.accounts}" var="item">
                <c:AccountRecord account="{!item}" />
            </aura:iteration>
         </div>
     </div>
    
    
    
</aura:component>